{
  "storage-driver": "{{ docker_storage_driver }}",
  "log-driver": "{{ docker_log_driver }}",
  "log-opts": {
{% for key, value in docker_log_opts.items() %}
    "{{ key }}": "{{ value }}"{% if not loop.last %},{% endif %}
{% endfor %}
  },
  "insecure-registries": [],
  "mtu": 1500,
  "dns": ["8.8.8.8", "8.8.4.4"],
  "default-runtime": "runc",
  "experimental": false,
  "features": {
    "buildkit": true
  }
}
